		"lint": "eslint src/ && tsc --noEmit",
		"prepare": "husky install",
		"prepublishOnly": "npm run build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Rei-666/discord-speech-recognition.git"
	},
	"keywords": [
		"discord.js",
		"discord",
		"speechrecognition"
	],
	"author": "Rei666",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Rei-666/discord-speech-recognition/issues"
	},
	"homepage": "https://discordsr.netlify.app/",
	"engines": {
		"node": ">=12.0.0"
	},
	"dependencies": {
		"axios": "^0.21.1",
		"discord.js": "^12.5.3",
		"node-fetch": "^2.6.1",
		"wav": "^1.0.2"
	},
	"devDependencies": {
		"@discordjs/opus": "^0.5.0",
		"@istanbuljs/nyc-config-typescript": "^1.0.1",
		"@types/chai": "^4.2.18",
		"@types/chai-as-promised": "^7.1.4",
		"@types/expect": "^24.3.0",
		"@types/mocha": "^8.2.2",
		"@types/node": "^15.0.2",
		"@types/node-fetch": "^2.5.10",
		"@types/wav": "^1.0.0",
		"@typescript-eslint/eslint-plugin": "^4.23.0",
		"@typescript-eslint/parser": "^4.23.0",
		"chai": "^4.3.4",
		"chai-as-promised": "^7.1.1",
		"dotenv": "^9.0.2",
		"eslint": "^7.26.0",
		"eslint-config-airbnb-typescript": "^12.3.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.24.1",
		"eslint-plugin-prettier": "^3.4.0",
		"ffmpeg-static": "^4.3.0",
		"husky": "^6.0.0",
		"mocha": "^8.4.0",
		"nodemon": "^2.0.7",
		"nyc": "^15.1.0",
		"opusscript": "0.0.8",
		"prettier": "2.3.2",
		"source-map-support": "^0.5.19",
		"ts-mocha": "^8.0.0",
		"ts-node": "^9.1.1",
		"typedoc": "^0.20.36",
		"typedoc-plugin-as-member-of": "^1.0.2",
		"typescript": "^4.2.4"
	},
	"files": [
		"dist/"
	]
