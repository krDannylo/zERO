	"description": "Extension for making discord speech recognition bots.",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"dev": "nodemon dist/index.js",
		"watch": "tsc -w",
		"doc": "typedoc",
		"build": "tsc",
		"test": "ts-mocha --exit --require source-map-support/register tests/**/*.test.ts ",
